{
  "SwaggerOptions": {
    "RoutePrefix": "private/docs/swagger",
    "JsonRoute": "private/docs/swagger/{documentName}/swagger.json",
    "Description": "Ukiyo API",
    "UIEndpoint": "v1/swagger.json"
  },
  "ConnectionStrings": {
    "Database": {
      "DBConn": "mongodb://localhost:27017",
      "DBName": "ukiyodb"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}